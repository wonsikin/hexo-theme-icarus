#backtotop
  height 50px
  position fixed
  bottom 55px
  right -60px
  z-index 9999
  cursor pointer
  -moz-transition all 1s ease
  -webkit-transition all 1s ease
  -o-transition all 1s ease
  transition all 1s ease

  .bttbg
    width 50px
    height 50px
    background url(images/top_button.png) no-repeat 0 0
#backtotop.showme
  right 12px
  -moz-transform scale(1) rotate(-720deg) translate(0px)
  -webkit-transform scale(1) rotate(-720deg) translate(0px)
  -o-transform scale(1) rotate(-720deg) translate(0px)
  transform scale(1) rotate(-720deg) translate(0px)
